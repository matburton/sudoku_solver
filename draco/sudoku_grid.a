getSquareOffset macro
	moveq     #0,d0
	move.w    4(sp),d0
    move.l    8(sp),a0
    mulu.w    2(a0),d0
    add.w     6(sp),d0
    mulu.w    14(a0),d0
	add.w     #20,d0
	endm

	xdef      getSquarePointer
